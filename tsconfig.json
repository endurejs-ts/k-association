{
    "compilerOptions": {
        "target": "ES2020",
        "module": "NodeNext",
        "lib": ["ES2015", "ES6", "DOM", "ES5"],
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strict": true,
        "noEmit": true,
        "declaration": true,
        "allowImportingTsExtensions": true,
        "esModuleInterop": true,
        "moduleResolution": "NodeNext",
        "rootDir": "."
    },
}